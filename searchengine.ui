<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Frame" id="mainwindow">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <child>
      <object class="tk.Entry" id="search_entry">
        <property name="justify">left</property>
        <property name="takefocus">true</property>
        <property name="validate">none</property>
        <property name="width">70</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="search_button">
        <property name="text" translatable="yes">search</property>
        <property name="width">10</property>
        <bind sequence="&lt;ButtonPress&gt;" handler="search_button_pressed" add="" />
        <layout manager="grid">
          <property name="column">1</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="speak_button">
        <property name="text" translatable="yes">speak</property>
        <property name="width">10</property>
        <bind sequence="&lt;ButtonPress&gt;" handler="speak_button_pressed" add="" />
        <layout manager="grid">
          <property name="column">2</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="response_textbox">
        <property name="font">TkIconFont</property>
        <property name="height">10</property>
        <property name="width">70</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="query_label">
        <property name="font">TkDefaultFont</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="help_search_button">
        <property name="compound">top</property>
        <property name="text" translatable="yes">help search</property>
        <property name="width">10</property>
        <bind sequence="&lt;ButtonPress&gt;" handler="help_search_button_pressed" add="" />
        <layout manager="grid">
          <property name="column">2</property>
          <property name="pady">10</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="help_speak_button">
        <property name="text" translatable="yes">help speak</property>
        <property name="width">10</property>
        <bind sequence="&lt;ButtonPress&gt;" handler="help_speak_button_pressed" add="" />
        <layout manager="grid">
          <property name="column">2</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
